{
    "files.exclude": {
        "umodel" : true,
        "*.exe": true,
        "*.bat": true,
        "*.{cfg,pdb,dll}": true,
        "package*.sh": true,
        "src-arc.sh": true,
        "**/.vs": true,
        "**/bak": true,
        "**/data": true,
        "**/Docs": true,
        "**/obj": true,
        "**/Tools/Max*": true,
        "**/Tools/Package*": true,
        "**/Tools/TypeInfo": true,
        "**/Tools/{UmdExtract,UT2down,unpack_obb.pl}": true,
        "**/UI/stl-stub": true,
        "**/UmodelExport": true,
        "**/UmodelSaved": true,
        "**/Screenshots": true
    },
    "files.associations": {
        "*.ush": "hlsl",
        "*.project": "perl"
    },
    "files.trimTrailingWhitespace": true,
    "C_Cpp.default.includePath": [
        "${workspaceFolder}",
        "${workspaceFolder}/Core",
        "${workspaceFolder}/Unreal",
        "${workspaceFolder}/UmodelTool",
        "${workspaceFolder}/libs",
        "${workspaceFolder}/libs/include",
        "${workspaceFolder}/libs/includewin32",
        "${workspaceFolder}/UI"
    ],
    "C_Cpp.default.defines": [
        "MAX_DEBUG",
        "RENDERING",
        "HAS_UI"
    ],
    "C_Cpp.default.forcedInclude": [
        "${workspaceFolder}/Unreal/GameDefines.h"
    ],
    "C_Cpp.default.cStandard": "c11",
    "C_Cpp.default.cppStandard": "c++17",
    "C_Cpp.default.intelliSenseMode": "msvc-x64",
    "C_Cpp.default.browse.path": [
        "${workspaceFolder}"
    ],
    "C_Cpp.default.browse.limitSymbolsToIncludedHeaders": true,
    "C_Cpp.default.browse.databaseFilename": "",
    "cSpell.words": [
		"FCOLOR",
		"FGPU",
		"FQUAT",
		"FROTATOR",
		"FVECTOR",
		"LODs",
		"SKEL",
		"SKELMESH",
		"Strncpyz",
		"Tris",
		"UMODEL",
		"coord",
		"kdop",
		"maxs",
		"tex",
		"uasset",
		"ubulk",
		"uexp",
		"unguardf",
		"unversioned",
		"recurse",
		"uv",
		"retargeting",
		"retarget"
	]
}
